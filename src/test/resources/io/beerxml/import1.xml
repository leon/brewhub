        <STYLE>
            <NAME>American IPA</NAME>
            <VERSION>1</VERSION>
            <CATEGORY>India Pale Ale (IPA)</CATEGORY>
            <CATEGORY_NUMBER>14</CATEGORY_NUMBER>
            <STYLE_LETTER>B</STYLE_LETTER>
            <STYLE_GUIDE>BJCP 2008</STYLE_GUIDE>
            <TYPE>Ale</TYPE>
            <OG_MIN>1.0560000</OG_MIN>
            <OG_MAX>1.0750000</OG_MAX>
